{
  "name": "@sked/preview-core",
  "version": "0.1.0",
  "description": "Core library for fetching URL metadata (OpenGraph)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "keywords": [
    "preview",
    "opengraph",
    "metadata",
    "url"
  ],
  "author": "Your Name <you@example.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "eslint": "^9.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
} 